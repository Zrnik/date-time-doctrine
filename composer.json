{
    "name": "brick/date-time-doctrine",
    "description": "Doctrine type mappings for brick/date-time",
    "type": "library",
    "keywords": [
        "Brick",
        "DateTime",
        "Doctrine"
    ],
    "license": "MIT",
    "require": {
        "brick/date-time": "~0.6.0",
        "doctrine/dbal": "^2.7.0 || ^3.0",
        "doctrine/orm": "^2.7.0",
        "php": "^8.1"
    },
    "require-dev": {
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^9.0",
        "php-coveralls/php-coveralls": "^2.4",
        "vimeo/psalm": "5.15.0",
        "doctrine/annotations": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Brick\\DateTime\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\DateTime\\Doctrine\\Tests\\": "tests/"
        }
    }
}
